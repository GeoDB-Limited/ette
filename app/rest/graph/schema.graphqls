type Block {
  hash: String!
  number: String!
  time: String!
  parentHash: String!
  difficulty: String!
  gasUsed: String!
  gasLimit: String!
  nonce: String!
  miner: String!
  size: Float!
  transactionRootHash: String!
  receiptRootHash: String!
}

type Query {
  block(hash: String!): Block!
}
